@model IEnumerable<Beauty.Data.Randevu>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tarih)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CidNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HnoNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UidNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tarih)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CidNavigation.Cid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HnoNavigation.Hno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UidNavigation.Uid)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
